CREATE TABLE champion
(
   id INT champion_id;
   name VARCHAR(50);
   lore VARCHAR(50);
   FOREIGN KEY(id_gender) REFERENCES gender(id_gender)
   FOREIGN KEY(id_year) REFERENCES year(id_year)
   FOREIGN KEY(id_resource) REFERENCES resource(id_resource)
    
)

