CREATE TABLE champions
(
   id INT champion_id;
   name VARCHAR(50);
   lore VARCHAR(50);
   Clé étrangère(gender_id) REFERENCES gender(gender_id)
   Clé étrangère(year_id) REFERENCES year(year_id)
   Clé étrangère(resource_id) REFERENCES resource(resource_id)
)
 
ALTER TABLE champions
ADD popularity INTEGER;

